
page {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  height: 100%;
  width: 100%;
  background: #f6f6f6;
}

.my-container{
  width: 100%;
}

.user-info{
  width: 100%;
  height: 180rpx;
  background-color: white;
  overflow: hidden
}

.user-info > image{
    width: 96rpx;
    height: 96rpx;
    margin: 42rpx 30rpx 42rpx 40rpx;
    border-radius: 50%;
    display: block;
    float: left;
}

.words-info{
  float: left;
  height: 96rpx;
  margin: 42rpx 0;
}

.nike-name{
  color: black;
  font-size: 26rpx;
}

.account{
  color: black;
  font-size: 26rpx;
  margin-top: 22rpx;
}

.my-nav-details{
  width: 100%;
  height: 342rpx;
  margin-top: 20rpx;
}

.my-nav{
    width: 100%;
    height: 85rpx;
    background-color: white;
}

.my-nav:not(:last-child) {
    border-bottom: 1rpx solid #e9e8ed;
}
.my-nav:last-child {
    border-bottom: 1rpx solid #e9e8ed;
}

.my-nav-info{
    height: 40rpx;
    padding: 22rpx 20rpx;
}

.my-nav-icon{
  width: 39rpx;
  height: 41rpx;
  display: block;
  float: left;
  margin-right: 24rpx;
}

.my-nav-info > .item-name{
  display: block;
  float: left;
  line-height: 42rpx;
}

.right-array {
    width: 16rpx;
    height: 25rpx;
    float: right;
    margin-top: 10rpx;
}

.my-waste{
   width: 100%;
    height: 85rpx;
    background-color: white;
    margin-top: 20rpx;
}

.my-setting{
    width: 100%;
    height: 85rpx;
    background-color: white;
    margin-top: 20rpx;
}


